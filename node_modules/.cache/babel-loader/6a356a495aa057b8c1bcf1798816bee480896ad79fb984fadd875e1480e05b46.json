{"ast":null,"code":"import{useTodoContext}from'../context/TodoContext';export const useTodos=()=>{const{dispatch,...rest}=useTodoContext();const addTodo=(text,priority)=>{if(text.trim()){dispatch({type:'ADD_TODO',payload:{text:text.trim(),priority}});}};const toggleTodo=id=>{dispatch({type:'TOGGLE_TODO',payload:id});};const deleteTodo=id=>{dispatch({type:'DELETE_TODO',payload:id});};const updateTodo=(id,updates)=>{dispatch({type:'UPDATE_TODO',payload:{id,updates}});};const setFilter=filter=>{dispatch({type:'SET_FILTER',payload:filter});};const setSearchTerm=term=>{dispatch({type:'SET_SEARCH_TERM',payload:term});};const setShowAddForm=show=>{dispatch({type:'SET_SHOW_ADD_FORM',payload:show});};const startEditing=(id,text)=>{dispatch({type:'START_EDITING',payload:{id,text}});};const cancelEditing=()=>{dispatch({type:'CANCEL_EDITING'});};const setEditValue=value=>{dispatch({type:'SET_EDIT_VALUE',payload:value});};const saveEdit=()=>{if(rest.editValue.trim()){updateTodo(rest.editingId,{text:rest.editValue.trim()});}};return{...rest,addTodo,toggleTodo,deleteTodo,updateTodo,setFilter,setSearchTerm,setShowAddForm,startEditing,cancelEditing,setEditValue,saveEdit};};","map":{"version":3,"names":["useTodoContext","useTodos","dispatch","rest","addTodo","text","priority","trim","type","payload","toggleTodo","id","deleteTodo","updateTodo","updates","setFilter","filter","setSearchTerm","term","setShowAddForm","show","startEditing","cancelEditing","setEditValue","value","saveEdit","editValue","editingId"],"sources":["/home/codeunity/Desktop/weshly/todo-app/src/hooks/useTodos.js"],"sourcesContent":["import { useTodoContext } from '../context/TodoContext';\n\nexport const useTodos = () => {\n  const { dispatch, ...rest } = useTodoContext();\n\n  const addTodo = (text, priority) => {\n    if (text.trim()) {\n      dispatch({ type: 'ADD_TODO', payload: { text: text.trim(), priority } });\n    }\n  };\n\n  const toggleTodo = (id) => {\n    dispatch({ type: 'TOGGLE_TODO', payload: id });\n  };\n\n  const deleteTodo = (id) => {\n    dispatch({ type: 'DELETE_TODO', payload: id });\n  };\n\n  const updateTodo = (id, updates) => {\n    dispatch({ type: 'UPDATE_TODO', payload: { id, updates } });\n  };\n\n  const setFilter = (filter) => {\n    dispatch({ type: 'SET_FILTER', payload: filter });\n  };\n\n  const setSearchTerm = (term) => {\n    dispatch({ type: 'SET_SEARCH_TERM', payload: term });\n  };\n\n  const setShowAddForm = (show) => {\n    dispatch({ type: 'SET_SHOW_ADD_FORM', payload: show });\n  };\n\n  const startEditing = (id, text) => {\n    dispatch({ type: 'START_EDITING', payload: { id, text } });\n  };\n\n  const cancelEditing = () => {\n    dispatch({ type: 'CANCEL_EDITING' });\n  };\n\n  const setEditValue = (value) => {\n    dispatch({ type: 'SET_EDIT_VALUE', payload: value });\n  };\n\n  const saveEdit = () => {\n    if (rest.editValue.trim()) {\n      updateTodo(rest.editingId, { text: rest.editValue.trim() });\n    }\n  };\n\n  return {\n    ...rest,\n    addTodo,\n    toggleTodo,\n    deleteTodo,\n    updateTodo,\n    setFilter,\n    setSearchTerm,\n    setShowAddForm,\n    startEditing,\n    cancelEditing,\n    setEditValue,\n    saveEdit,\n  };\n};"],"mappings":"AAAA,OAASA,cAAc,KAAQ,wBAAwB,CAEvD,MAAO,MAAM,CAAAC,QAAQ,CAAGA,CAAA,GAAM,CAC5B,KAAM,CAAEC,QAAQ,CAAE,GAAGC,IAAK,CAAC,CAAGH,cAAc,CAAC,CAAC,CAE9C,KAAM,CAAAI,OAAO,CAAGA,CAACC,IAAI,CAAEC,QAAQ,GAAK,CAClC,GAAID,IAAI,CAACE,IAAI,CAAC,CAAC,CAAE,CACfL,QAAQ,CAAC,CAAEM,IAAI,CAAE,UAAU,CAAEC,OAAO,CAAE,CAAEJ,IAAI,CAAEA,IAAI,CAACE,IAAI,CAAC,CAAC,CAAED,QAAS,CAAE,CAAC,CAAC,CAC1E,CACF,CAAC,CAED,KAAM,CAAAI,UAAU,CAAIC,EAAE,EAAK,CACzBT,QAAQ,CAAC,CAAEM,IAAI,CAAE,aAAa,CAAEC,OAAO,CAAEE,EAAG,CAAC,CAAC,CAChD,CAAC,CAED,KAAM,CAAAC,UAAU,CAAID,EAAE,EAAK,CACzBT,QAAQ,CAAC,CAAEM,IAAI,CAAE,aAAa,CAAEC,OAAO,CAAEE,EAAG,CAAC,CAAC,CAChD,CAAC,CAED,KAAM,CAAAE,UAAU,CAAGA,CAACF,EAAE,CAAEG,OAAO,GAAK,CAClCZ,QAAQ,CAAC,CAAEM,IAAI,CAAE,aAAa,CAAEC,OAAO,CAAE,CAAEE,EAAE,CAAEG,OAAQ,CAAE,CAAC,CAAC,CAC7D,CAAC,CAED,KAAM,CAAAC,SAA